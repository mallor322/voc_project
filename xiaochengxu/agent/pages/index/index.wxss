/* agent.wxss */
.weui-tab__panel{
  background: #f4f4f4;
  padding-top:120rpx;
}
.weui-navbar{
  background: #fff
}
.weui-panel__hd{
  padding:20rpx 30rpx;
}
.weui-media-box{
  padding: 24rpx 30rpx
}
.weui-panel__hd::after{
  left:0rpx;
}
.weui-media-box__hd_in-appmsg {
  margin-right:30rpx;
  width:250rpx;
  height:200rpx;
  line-height:200rpx;
  text-align:center;
}
.weui-media-box__desc{
  line-height: 1.3;
}
.weui-search-bar{
  border:none;
  background: #f4f4f4;
}
.nodata{
  position: relative;
  text-align: center;
  margin-top:40rpx;
}
.nodata .left{
  border-top: 2rpx solid #ddd;
  position: relative;
  top:-8rpx;
  height:2rpx;
  width:150rpx;
  margin-right: 30rpx;
  display: inline-block;
}
.nodata .right{
  border-top: 2rpx solid #ddd;
  position: relative;
  top:-8rpx;
  height:2rpx;
  width:150rpx;
  margin-left: 30rpx;
  display: inline-block;
}
.nodata .text{
  color: #999;
  display: inline-block;
}
page{
  background: #f4f4f4;
}