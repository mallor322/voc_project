.reserve-form {
  padding: 100rpx 40rpx 20rpx;
}
.reserve-form .form-input {
  border-bottom: 1rpx solid #ddd;
}
.reserve-form .form-input text {
  color: #111;
  height: 100rpx;
  line-height: 100rpx;
  width: 160rpx;
  font-size: 30rpx;
  float:left;
}
.reserve-form .form-input input {
  color: #111;
  display: block;
  height: 100rpx;
  line-height: 100rpx;
  width: calc( 100% - 160rpx);
  font-size: 30rpx;
  font-family: "Microsoft Yahei";
  float:left;
  box-sizing:border-box;
}
.reserve-form .form-btn {
  margin: 88rpx auto 0;
}
.default-btn{
  height:auto;
}
.placeholder{
  color: #999;
  font-weight: normal;
  font-family: "Microsoft Yahei";
}
.clear{
  clear: both;
}
page{
  background: #f4f4f4;
}
.logo{
  margin-top: 100rpx;
  height: 200rpx;
  width: 100%;
  text-align: center;
}
.logo-outer{
  height: 200rpx;
  width: 200rpx;
  border-radius: 100rpx;
  display: inline-block;
}
.logo-img{
  width: 100%;
  height: 100%;
  border-radius: 100rpx;
}
