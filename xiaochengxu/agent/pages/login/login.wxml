
<view class="pages">
    <view class="logo">
        <view class="logo-outer">
            <image class="logo-img" src="../../images/logo.jpg" />
        </view>
    </view>
    <form bindsubmit="formsubmit">
      <view class="reserve-form">
        <view class="form-input">
          <text>账号</text>
          <input type="text" name="phone" placeholder-class="placeholder" placeholder="请输入账号" />
          <view class="clear"></view>
        </view>
        <view class="form-input">
          <text>密码</text>
          <input type="text" password="true" name="password" placeholder-class="placeholder" placeholder="请输入密码" />
          <view class="clear"></view>
        </view>
        <view class="form-btn">
          <view class="default-btn">
            <button type="primary" form-type="submit" disabled="{{disabled}}">登录</button>
          </view>
        </view>
      </view>
    </form>
</view>