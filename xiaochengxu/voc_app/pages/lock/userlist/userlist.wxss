.panel-title{
    padding-left: 15px;
    position: relative;
    top:2px;
    margin-top: 30rpx;
    color:#9c9c9c;
    font-size: 30rpx;
}
.title{
    color: #666666;
    font-size: 32rpx;
}
.main{
    color: #ec6500;
    text-align:right;
}
.avater{
    height: 110rpx;
    line-height: 110rpx;
}
.avater .text{
    font-size: 28rpx;
    line-height: 38rpx;
    display: inline-block;
    float: left;
    padding-left: 20rpx;
    color:#666666;
}
.avater .userinfo-avatar{
    margin: 0rpx;
    float: left;
    width: 110rpx;
    height: 110rpx;
    border-radius: 50%;
}
.weui-cell{
    padding: 28rpx 30rpx;
}