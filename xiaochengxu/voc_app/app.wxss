/**app.wxss**/
@import 'assets/wxss/weui.wxss';

page{
    background-color: #f1f1f1;
    font-family: Microsoft yahei;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.logo-page{
    padding-top: 110rpx;
}
.logoinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.logoinfo-avatar {
  width: 160rpx;
  height: 160rpx;
  margin: 20rpx;
  border-radius: 50%;
}
.eye{
    position: absolute;
    right: 10rpx;
    bottom: 28rpx;
    color: #ddd;
}
.logo-form {
  padding: 80rpx 40rpx 20rpx;
}
.logo-form .form-input {
  border-bottom: 1rpx solid #e3e3e3;
  position: relative;
}
.logo-form .form-input input {
  color: #111;
  display: block;
  height: 140rpx;
  line-height: 140rpx;
  width: calc( 100% - 110rpx);
  font-size: 30rpx;
  font-family: "Microsoft Yahei";
  float:left;
  box-sizing:border-box;
}
.logo-form .form-input input.pwdcss{
   width: calc( 100% - 180rpx);
}
.logo-form .form-input .icon{
    color: #666;
    height: 140rpx;
    line-height: 140rpx;
    width: 110rpx;
    font-size: 28rpx;
    float: left;
}
.logo-form .form-input .icon text{
    position:relative;
    top: -8rpx;
    left:20rpx;
    color: #bdbdbd;
}
.logo-form .form-btn {
  margin: 88rpx auto 0;
}
.placeholder{
  color: #999;
  font-weight: normal;
  font-family: "Microsoft Yahei";
}
.clear{
  clear: both;
}
.tologin{
    background-color:#ed6501;
    opacity: 0.6;
}
.forget{
    margin-top: 30rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    color: #aaa;
    font-size: 28rpx;
}
.regist{
    font-size: 28rpx;
    width: calc( 100% - 60rpx );
    position: fixed;
    bottom:30rpx;
    left:0rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    color: #ed6501;
    padding-top: 30rpx;
    margin-left: 30rpx;
    margin-right: 30rpx;
    border-top: 1rpx solid #e3e3e3;
}
.login-btn{
    color: #fff;
    background: #ed6501;
    border-color: #ed6501;
    border-radius: 50rpx;
}
button[disabled][type="default"], wx-button[disabled]:not([type]) {
    color:#fff;
    background-color:#ed6501;
    opacity: 0.3;
}
.empty{
    height: 30rpx;
    width: 100%;
}
.empty-top{
    height: 20rpx;
    width: 100%;
}
.update-page{
    padding-top: 30rpx;
    position: relative;
}
.update-item{
    border-top: 1rpx solid #e1e1e1;
    border-bottom: 1rpx solid #e1e1e1;
    font-size: 30rpx;
    padding: 20rpx 30rpx;
    background:#fff;
}
.placeholder{
    color: #9c9c9c;
}
.save-bg{
    padding:30rpx;
    margin-top:30rpx;
}
.radio-value{
    color: #666;
}
.default-color{
    color: #666;
}
.first-color{
    color: #333;
}
.center{
    align-content: center;
}



@font-face {
	font-family: 'iconfont';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: 500;
